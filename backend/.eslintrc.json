{
  "extends": "./node_modules/mwts/",
  "ignorePatterns": ["node_modules", "dist", "test", "jest.config.js", "typings"],
  "env": {
    "jest": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", {
      "argsIgnorePattern": "^_"
    }],
    "node/no-unpublished-import": [
      "warn",
      {
        "allowModules": [
          "prisma"
        ]
      }
    ]
  }
}
